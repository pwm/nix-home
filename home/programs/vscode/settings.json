{
  "breadcrumbs.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.detectIndentation": false,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": false,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.hover.delay": 150,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": false,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Default Dark+]": {
      "comments": "#aeb3a4d0"
    }
  },
  "editor.wordWrapColumn": 120,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoUpdate": false,
  "files.autoSave": "afterDelay",
  "files.trimTrailingWhitespace": true,
  "gitlens.codeLens.enabled": false,
  "haskell.formattingProvider": "ormolu",
  "haskell.indentationRules.enabled": true,
  "haskell.logFile": "",
  "haskell.plugin.ormolu.config.external": true,
  "haskell.serverExecutablePath": "haskell-language-server",
  "haskell.trace.server": "off",
  "nix.enableLanguageServer": true,
  "nix.formatterPath": "nixpkgs-fmt",
  "redhat.telemetry.enabled": false,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "security.workspace.trust.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.allowChords": false,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.fontFamily": "Fira Code",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.ignoreBracketedPasteMode": true,
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.profiles.osx": {
    "fish": {
      "args": [
        "-l"
      ],
      "path": "/Users/pwm/.nix-profile/bin/fish"
    }
  },
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.sendKeybindingsToShell": false,
  "terminal.integrated.tabs.hideCondition": "singleGroup",
  "update.mode": "none",
  "window.restoreWindows": "all",
  "window.zoomLevel": 1,
  "workbench.colorCustomizations": {
    "editorError.border": "#ff0000",
    "editorError.foreground": "#ff0000",
    "editorHint.border": "#ffa500",
    "editorHint.foreground": "#ffa500",
    "editorInfo.border": "#ffa500",
    "editorInfo.foreground": "#ffa500",
    "editorWarning.border": "#ff4500",
    "editorWarning.foreground": "#ff4500"
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.panel.defaultLocation": "right",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "none",
  "yaml.format.enable": true,
  "yaml.format.printWidth": 160
}